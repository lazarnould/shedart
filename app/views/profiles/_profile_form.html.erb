<%= simple_form_for [@profile] do |f| %>
  <%= f.error_notification %>

  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :country %>
  <%= f.input :address %>
  <%= f.input :zip_code %>
  <%= f.input :city %>
  <%= f.input :phone_number %>
  <%= f.input :photo, label: 'Add your Head Picture' %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.button :submit %>
<% end %>





